Algoritmo "prestção"

Var
    compra , prestacao : real


Inicio
       escreva( "Loja Mamão Com Açucar " )
       escreva( " Valor da sua Compra = " )
       leia(compra)
       prestacao <- compra/5
       escreval("O cliente deverá pagar 5 prestações de R$",prestacao," cada")


Fimalgoritmo